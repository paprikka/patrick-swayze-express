{
    "extends": "airbnb",
    "plugins": [
        "jest"
    ],
    "globals": {
      "spyOn": true
    },
    "env": {
      "jest/globals": true
    },
    "rules": {
      "import/prefer-default-export": "off",
      "semi": [ "error", "never" ],
      "indent": ["error", 4],
      "default-case": 0,
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "array-bracket-spacing": [
        "error",
        "always"
      ],
      "space-in-parens": [
        "error",
        "always"
      ]
    }
  }